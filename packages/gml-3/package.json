{
  "name": "geojson-to-gml-3",
  "version": "2.0.2",
  "description": "a package to translate geojson to GML 3.2.1",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "src/index.ts",
    "dist/index.cjs",
    "dist/index.mjs",
    "dist/index.d.ts",
    "LICENSE.md",
    "license/PolyForm-Noncommercial-1.0.0.md",
    "license/PolyForm-Noncommercial-1.0.0.md",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/skalt/geox.git",
    "directory": "packages/geojson-to-gml-3"
  },
  "scripts": {
    "test": "vitest",
    "make-docs": "node make-docs.js",
    "build:cjs": "esbuild --format=cjs --out-extension:.js=.cjs --target=es2021 --tsconfig=./tsconfig.json --minify --sourcemap --outdir=dist ./src/index.ts",
    "build:mjs": "esbuild --format=esm --out-extension:.js=.mjs --target=es2021 --tsconfig=./tsconfig.json --minify --sourcemap --outdir=dist ./src/index.ts",
    "build": "pnpm build:cjs && pnpm build:mjs"
  },
  "keywords": [
    "geojson",
    "geoserver",
    "GIS",
    "GML",
    "xml"
  ],
  "author": "Steven Kalt",
  "license": "SEE LICENSE IN ./LICENSE.md",
  "dependencies": {
    "minimxml": "workspace:minimxml"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.14",
    "@vitest/coverage-v8": "^1.6.0",
    "prettier": "^3.3.3",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-doctest": "^1.0.0",
    "vitest": "^1.6.0"
  }
}
